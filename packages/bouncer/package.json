{
  "name": "@tanstack/bouncer",
  "version": "0.0.1-alpha.0",
  "description": "Utilities for debouncing, throttling, scheduling, queuing, and more.",
  "author": "Tanner Linsley",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/bouncer.git",
    "directory": "packages/bouncer"
  },
  "homepage": "https://tanstack.com/bouncer",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/tannerlinsley"
  },
  "keywords": [
    "debounce",
    "throttle",
    "schedule",
    "queue",
    "bouncer"
  ],
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./async-throttler": {
      "import": {
        "types": "./dist/esm/async-throttler.d.ts",
        "default": "./dist/esm/async-throttler.js"
      },
      "require": {
        "types": "./dist/cjs/async-throttler.d.cts",
        "default": "./dist/cjs/async-throttler.cjs"
      }
    },
    "./debouncer": {
      "import": {
        "types": "./dist/esm/debouncer.d.ts",
        "default": "./dist/esm/debouncer.js"
      },
      "require": {
        "types": "./dist/cjs/debouncer.d.cts",
        "default": "./dist/cjs/debouncer.cjs"
      }
    },
    "./throttler": {
      "import": {
        "types": "./dist/esm/throttler.d.ts",
        "default": "./dist/esm/throttler.js"
      },
      "require": {
        "types": "./dist/cjs/throttler.d.cts",
        "default": "./dist/cjs/throttler.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist/",
    "src"
  ],
  "scripts": {
    "clean": "rimraf ./build && rimraf ./dist",
    "lint:fix": "eslint ./src --fix",
    "test:eslint": "eslint ./src",
    "test:lib": "vitest",
    "test:lib:dev": "pnpm test:lib --watch",
    "test:types": "tsc",
    "test:build": "publint --strict",
    "build": "vite build"
  }
}
